{
    "name": "@inspectai/database",
    "version": "0.1.0",
    "private": true,
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "scripts": {
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
