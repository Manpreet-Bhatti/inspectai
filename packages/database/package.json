        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@prisma/adapter-pg": "^7.1.0",
        "@prisma/client": "^7.1.0",
        "dotenv": "^16.5.0"
    },
    "devDependencies": {
        "dotenv-cli": "^8.0.0",
        "prisma": "^7.1.0",
        "tsx": "^4.20.3",
        "typescript": "^5.9.3"
    }
}