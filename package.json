{
    "name": "inspectai",
    "version": "0.1.0",
    "private": true,
    "packageManager": "pnpm@10.26.0",
    "scripts": {
        "dev": "turbo dev",
        "build": "turbo build",
        "lint": "turbo lint",
        "test": "turbo test",
        "test:e2e": "turbo test:e2e",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
        "db:push": "supabase db push",
        "db:reset": "supabase db reset",
        "db:types": "supabase gen types typescript --local > apps/web/types/database.ts",
        "clean": "turbo clean && rm -rf node_modules"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "prettier": "^3.7.4",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "turbo": "^2.6.2",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=22.0.0"
    }
}